package Print;

import java.io.PrintWriter;

public class Writer {
	
	public static void main(String[] args) {
		
		PrintWriter pw = new PrintWriter (System.out, true);
		
		String createmyownprinter = "Project 2";
		
		pw.println("This is " + createmyownprinter + ". Yay, but what was the point?");
		
		// what is the benefit of creating your own print method?
		
	}

}
